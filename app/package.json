    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "postcss": "^8.5.3",
    "shadcn": "2.4.0-canary.11",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.4",
    "typescript": "~5.8.2",
