{
  "name": "@plateunlp/app",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "with-env": "dotenvx run -f .env.local -f .env --",
    "with-env-prod": "dotenvx run -f .env.local -f .env --env NODE_ENV=production --",
    "dev": "pnpm with-env vinxi dev",
    "build": "pnpm with-env-prod vinxi build",
    "start": "pnpm with-env-prod vinxi start --host",
    "check-types": "pnpm with-env tsc --noEmit",
    "lint": "pnpm with-env eslint .",
    "lint:fix": "pnpm with-env eslint . --fix",
    "db:generate": "pnpm with-env drizzle-kit generate --name",
    "db:migrate": "pnpm with-env drizzle-kit migrate",
    "db:seed": "pnpm with-env tsx server/db/seed.ts",
    "db:dev": "pnpm db:migrate && pnpm db:seed"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.43.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^4.1.3",
